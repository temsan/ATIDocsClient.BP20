<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://docs.ati.su/xml/ns/0.1/" targetNamespace="http://docs.ati.su/xml/ns/0.1/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="document">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="header">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="title" type="xs:string"/>
							<xs:element name="number" type="xs:string" nillable="true" minOccurs="0"/>
							<xs:element name="date" type="xs:date"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="body">
					<xs:complexType>
						<xs:all minOccurs="0">
							<xs:element name="relation" nillable="true" minOccurs="0">
								<xs:complexType>
									<xs:sequence minOccurs="0">
										<xs:element name="number" type="xs:string"/>
										<xs:element name="date" type="xs:string"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="header">
								<xs:complexType>
									<xs:all>
										<xs:element name="customer">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="cn"/>
													<xs:element name="text"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="performer">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="cn"/>
													<xs:element name="text"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="para" type="xs:string" minOccurs="0"/>
										<xs:element name="number" type="xs:string" nillable="true" minOccurs="0"/>
										<xs:element name="date" type="xs:string" nillable="true" minOccurs="0"/>
										<xs:element name="notes" type="xs:string"/>
									</xs:all>
								</xs:complexType>
							</xs:element>
							<xs:element name="route">
								<xs:complexType>
									<xs:sequence minOccurs="0" maxOccurs="unbounded">
										<xs:element name="location" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:all>
													<xs:element name="action">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="method" type="xs:string" use="optional"/>
																	<xs:attribute name="type" type="xs:string"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="address">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="city" type="xs:string"/>
																<xs:element name="locality" type="xs:string"/>
																<xs:element name="latitude" type="xs:double" minOccurs="0"/>
																<xs:element name="longitude" type="xs:double" minOccurs="0"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="date" type="xs:date"/>
													<xs:element name="dateFrom" nillable="true" minOccurs="0"/>
													<xs:element name="dateTo" type="xs:string" nillable="true" minOccurs="0"/>
													<xs:element name="time" type="xs:string" nillable="true"/>
													<xs:element name="duration" type="xs:string"/>
													<xs:element name="agent">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="title" type="xs:string"/>
																<xs:element name="contact" type="xs:string"/>
															</xs:sequence>
															<xs:attribute name="role" type="xs:string" use="optional"/>
														</xs:complexType>
													</xs:element>
													<xs:element name="list" minOccurs="0">
														<xs:complexType>
															<xs:sequence minOccurs="0" maxOccurs="unbounded">
																<xs:element name="cargo">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="name" type="xs:string" nillable="true" minOccurs="0"/>
																			<xs:element name="amount" type="xs:string" default="" nillable="true" minOccurs="0"/>
																			<xs:element name="package" type="xs:string" nillable="true" minOccurs="0"/>
																			<xs:element name="weight" default="0" nillable="true" minOccurs="0">
																				<xs:complexType mixed="false">
																					<xs:simpleContent>
																						<xs:extension base="xs:string">
																							<xs:attribute name="unit" type="xs:string"/>
																						</xs:extension>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="volume" minOccurs="0">
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string">
																							<xs:attribute name="unit" type="xs:string"/>
																						</xs:extension>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="dimensions">
																				<xs:complexType>
																					<xs:simpleContent>
																						<xs:extension base="xs:string">
																							<xs:attribute name="unit" type="xs:string"/>
																						</xs:extension>
																					</xs:simpleContent>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="cost" type="xs:string" nillable="true" minOccurs="0"/>
																			<xs:element name="adr" type="xs:string" nillable="true" minOccurs="0"/>
																			<xs:element name="requirements" type="xs:string" nillable="true" minOccurs="0"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
															<xs:attribute name="role" type="xs:string"/>
														</xs:complexType>
													</xs:element>
												</xs:all>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="path" type="xs:string"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="transport">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="requirements">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="capacity" nillable="true" minOccurs="0">
														<xs:complexType>
															<xs:simpleContent>
																<xs:extension base="xs:string">
																	<xs:attribute name="unit" type="xs:string"/>
																</xs:extension>
															</xs:simpleContent>
														</xs:complexType>
													</xs:element>
													<xs:element name="type" type="xs:string"/>
													<xs:element name="options">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="option" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="notes" type="xs:string"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="list">
											<xs:complexType>
												<xs:sequence maxOccurs="unbounded">
													<xs:element name="vehicle" nillable="true" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="tractor" nillable="true">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="brand" type="xs:string" nillable="true"/>
																			<xs:element name="model" type="xs:string" nillable="true"/>
																			<xs:element name="regnum" type="xs:string" nillable="true" minOccurs="0"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="trailer">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="brand" type="xs:string" nillable="true"/>
																			<xs:element name="model" type="xs:string" nillable="true"/>
																			<xs:element name="regnum" type="xs:string" nillable="true" minOccurs="0"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="drivers">
								<xs:complexType>
									<xs:sequence minOccurs="0" maxOccurs="unbounded">
										<xs:element name="driver" nillable="true" minOccurs="0">
											<xs:complexType>
												<xs:all minOccurs="0">
													<xs:element name="name" type="xs:string" nillable="true" minOccurs="0"/>
													<xs:element name="passport" type="xs:string" nillable="true" minOccurs="0"/>
													<xs:element name="mobile" type="xs:string" nillable="true" minOccurs="0"/>
													<xs:element name="driverLicense" type="xs:string" nillable="true" minOccurs="0"/>
												</xs:all>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="payment">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="header" type="xs:string"/>
										<xs:element name="parts" minOccurs="0">
											<xs:complexType>
												<xs:sequence minOccurs="0" maxOccurs="unbounded">
													<xs:element name="part" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="value" type="xs:string" nillable="true" minOccurs="0"/>
																<xs:element name="method" type="xs:string"/>
																<xs:element name="currency" type="xs:string"/>
																<xs:element name="dueDate" nillable="true" minOccurs="0"/>
																<xs:element name="docs">
																	<xs:complexType>
																		<xs:simpleContent>
																			<xs:extension base="xs:string">
																				<xs:attribute name="type" type="xs:string"/>
																			</xs:extension>
																		</xs:simpleContent>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
															<xs:attribute name="type" type="xs:string"/>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="total" nillable="true" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="name" type="xs:string"/>
													<xs:element name="value" type="xs:decimal"/>
													<xs:element name="timeForTransport" type="xs:string" nillable="true" minOccurs="0"/>
													<xs:element name="minWorkTime" type="xs:string" nillable="true" minOccurs="0"/>
													<xs:element name="unit" type="xs:string"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="note" type="xs:string" nillable="true" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="additional">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="header" type="xs:string"/>
										<xs:element name="agreement" nillable="true" minOccurs="0">
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:string"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="legalAgreement" type="xs:string"/>
							<xs:element name="penalties" nillable="true" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="header" type="xs:string"/>
										<xs:element name="text" type="xs:string"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:all>
					</xs:complexType>
				</xs:element>
				<xs:element name="agents">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="agent" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ownership" type="xs:string"/>
										<xs:element name="title" type="xs:string"/>
										<xs:element name="contact" type="xs:string" nillable="true" minOccurs="0"/>
										<xs:element name="requisites" type="xs:string"/>
									</xs:sequence>
									<xs:attribute name="role" type="xs:string" use="optional"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="id" type="xs:string"/>
			<xs:attribute name="type" type="xs:string"/>
			<xs:attribute name="version" type="xs:float"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
